{
  "name": "nuxt3-template",
  "description": "Opinionated Nuxt 3 Starter Template",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.5.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pretty": "prettier --write \"./src/**/*.{js,ts,vue,css}\"",
    "test": "vitest run",
    "unlighthouse": "unlighthouse"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@nuxt/content": "^3.3.0",
    "@nuxt/fonts": "^0.10.3",
    "@nuxt/image": "^1.9.0",
    "@nuxt/ui": "^2.21.1",
    "@nuxtjs/i18n": "^9.3.0",
    "@pinia/nuxt": "^0.10.1",
    "@vueuse/nuxt": "^12.8.2",
    "nuxt": "^3.16.0",
    "nuxt-headlessui": "^1.2.0",
    "pinia": "^3.0.1"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.1.0",
    "@nuxt/test-utils": "^3.17.1",
    "@nuxtjs/html-validator": "^2.1.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.22.0",
    "happy-dom": "^17.4.1",
    "playwright-core": "^1.51.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "unlighthouse": "^0.16.2",
    "vitest": "^3.0.8"
  }
}