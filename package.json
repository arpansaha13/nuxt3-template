{
  "name": "nuxt3-template",
  "description": "Opinionated Nuxt 3 Starter Template",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.5.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pretty": "prettier --write \"./**/*.{js,ts,vue,css}\" \"!**/.nuxt/**\"",
    "test": "vitest run",
    "unlighthouse": "unlighthouse"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "nuxt": "^3.15.4",
    "pinia": "^2.3.1"
  },
  "devDependencies": {
    "@nuxt/content": "^2.13.4",
    "@nuxt/eslint": "^0.7.6",
    "@nuxt/fonts": "^0.0.1",
    "@nuxt/image": "^1.9.0",
    "@nuxt/test-utils": "^3.17.0",
    "@nuxt/ui": "^2.21.0",
    "@nuxtjs/html-validator": "^1.8.2",
    "@nuxtjs/i18n": "^8.5.6",
    "@pinia/nuxt": "^0.5.5",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/nuxt": "^10.11.1",
    "eslint": "^9.21.0",
    "happy-dom": "^17.3.0",
    "nuxt-headlessui": "^1.2.0",
    "playwright-core": "^1.51.0",
    "prettier": "^2.8.8",
    "typescript": "^5.8.2",
    "unlighthouse": "^0.11.6",
    "vitest": "^3.0.8"
  }
}