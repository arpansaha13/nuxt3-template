{
  "name": "nuxt3-template",
  "description": "Opinionated Nuxt 3 Starter Template",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pretty": "prettier --write \"./src/**/*.{js,ts,vue,css}\"",
    "test": "vitest run",
    "unlighthouse": "unlighthouse"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/lucide": "^1.2.30",
    "@iconify-json/simple-icons": "^1.2.28",
    "@nuxt/content": "^3.3.0",
    "@nuxt/image": "^1.9.0",
    "@nuxt/ui": "^3.0.0",
    "@pinia/nuxt": "^0.10.1",
    "@vueuse/nuxt": "^13.0.0",
    "nuxt": "^3.16.0",
    "pinia": "^3.0.1",
    "reka-ui": "^2.0.2",
    "tailwindcss": "^4.0.14",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.1.0",
    "@nuxt/test-utils": "^3.17.1",
    "@nuxtjs/html-validator": "^2.1.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.22.0",
    "happy-dom": "^17.4.1",
    "playwright-core": "^1.51.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.8.2",
    "unlighthouse": "^0.16.2",
    "vitest": "^3.0.8"
  }
}